arents = res.data\r\n    }\r\n  },\r\n  created(){\r\n      this.fetchParents()\r\n      this.id && this.fetch()\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views"}]}