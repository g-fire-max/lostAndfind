AFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA,KAzBA;AA0BA,IAAA,YA1BA,0BA0BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,GAAA,cADA;;AAAA;AACA,gBAAA,GADA;AAEA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,gBAAA,MAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA;AA9BA,GAXA;AA2CA,EAAA,OA3CA,qBA2CA;AACA,SAAA,YAAA;AACA,SAAA,EAAA,IAAA,KAAA,KAAA,EAAA;AACA;AA9CA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <h1>{{id ? '编辑': '新建'}}招领</h1>\r\n    <el-form  label-width=\"120px\" @submit.native.prevent=\"save\">\r\n      <el-form-item label=\"拾到的物品名\" >\r\n            <el-input v-model=\"model.name\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"拾物区域\" prop=\"region\">\r\n        <el-select v-model=\"model.region\" placeholder=\"请选择丢失区域\">\r\n            <el-option label=\"一教\" value=\"一教\"></el-option>\r\n            <el-option label=\"二教\" value=\"二教\"></el-option>\r\n            <el-option label=\"一食堂\" value=\"一食堂\"></el-option>\r\n            <el-option label=\"二食堂\" value=\"二食堂\"></el-option>\r\n            <el-option label=\"图书馆\" value=\"图书馆\"></el-option>\r\n        </el-select>\r\n        </el-form-item>\r\n            <el-form-item label=\"拾物类型\" prop=\"classify\">\r\n               <el-select v-model=\"model.classify\" placeholder=\"请选择拾物类型\">\r\n                 <el-option label=\"日用品\" value=\"日用品\"></el-option>\r\n                 <el-option label=\"电子用品\" value=\"电子用品\"></el-option>\r\n                 <el-option label=\"学习用品\" value=\"学习用品\"></el-option>\r\n               </el-select>\r\n         </el-form-item>\r\n          <el-form-item label=\"拾物时间\" required>\r\n          <el-col :span=\"11\">\r\n             <el-form-item prop=\"time\">\r\n              <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"model.time\" style=\"width: 100%;\"></el-date-picker>\r\n             </el-form-item>\r\n             </el-col>\r\n          </el-form-item>\r\n          <el-form-item label=\"描述拾到物品\" prop=\"desc\">\r\n              <el-input type=\"textarea\" v-model=\"model.desc\"></el-input>\r\n           </el-form-item>\r\n         <el-form-item label=\"联系方式\" prop=\"tel\">\r\n           <el-input v-model=\"model.tel\"></el-input>\r\n         </el-form-item>\r\n        <el-form-item label=\"图标\">\r\n            <el-upload\r\n              class=\"avatar-uploader\"\r\n              :action=\"$http.defaults.baseURL + '/upload'\" \r\n              :show-file-list=\"false\"\r\n              :on-success=\"afterUpload\">\r\n              <img v-if=\"model.img\" :src=\"model.img\" class=\"avatar\">\r\n              <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n            </el-upload>\r\n        </el-form-item>\r\n        <el-form-item>\r\n            <el-button type=\"primary\" native-type=\"submit\">保存</el-button>\r\n        </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name:'FindEdit',\r\n  props:{\r\n      id:{}\r\n  },\r\n  data() {\r\n    return {\r\n      model:{},\r\n      parents:[],\r\n    }\r\n  },\r\n  methods: {\r\n     afterUpload(res){\r\n       console.log(res)\r\n      //显示赋值\r\n       this.$set(this.model,'img',res.url)\r\n    //    this.model.icon = res.url\r\n      },\r\n   async save(){\r\n       let res \r\n       if(this.id) {\r\n         res = await this.$http.put(`rest/finds/${this.id}`,this.model)\r\n          // console.log(111);\r\n          // console.log(res)\r\n       }else {\r\n          res = await this.$http.post('rest/finds',this.model)\r\n       }\r\n        this.$router.push('/finds/list')\r\n        this.$message({\r\n            type:'success',\r\n            message:'保存成功'\r\n        })\r\n     },\r\n    async fetch() {\r\n        const res = await this.$http.get(`rest/finds/${this.id}`)\r\n        this.model = res.data\r\n    },\r\n    async fetchParents() {\r\n      const res = await this.$http.get(`rest/finds`)\r\n      console.log(res)\r\n      this.parents = res.data\r\n    }\r\n  },\r\n  created(){\r\n      this.fetchParents()\r\n      this.id && this.fetch()\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .avatar-uploader .el-upload {\r\n    border: 1px dashed #d9d9d9;\r\n    border-radius: 6px;\r\n    cursor: pointer;\r\n    position: relative;\r\n    overflow: hidden;\r\n  }\r\n  .avatar-uploader .el-upload:hover {\r\n    border-color: #409EFF;\r\n  }\r\n  .avatar-uploader-icon {\r\n    font-size: 28px;\r\n    color: #8c939d;\r\n    width: 178px;\r\n    height: 178px;\r\n    line-height: 178px;\r\n    text-align: center;\r\n  }\r\n  .avatar {\r\n    width: 178px;\r\n    height: 178px;\r\n    display: block;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/views"}]}