form>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name:'ItemEdit',\r\n  props:{\r\n      id:{}\r\n  },\r\n  data() {\r\n    return {\r\n      model:{},\r\n    }\r\n  },\r\n  methods: {\r\n      afterUpload(res){\r\n      //  console.log(res)\r\n      //显示赋值\r\n       this.$set(this.model,'icon',res.url)\r\n    //    this.model.icon = res.url\r\n      },\r\n   async save(){\r\n       let res \r\n       if(this.id) {\r\n         res = await this.$http.put(`rest/items/${this.id}`,this.model)\r\n          // console.log(111);\r\n          // console.log(res)\r\n       }else {\r\n          res = await this.$http.post('rest/items',this.model)\r\n       }\r\n        this.$router.push('/items/list')\r\n        this.$message({\r\n            type:'success',\r\n            message:'保存成功'\r\n        })\r\n     },\r\n    async fetch() {\r\n        const res = await this.$http.get(`rest/items/${this.id}`)\r\n        this.model = res.data\r\n    },\r\n  },\r\n  created(){\r\n      this.id && this.fetch()\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n  .avatar-uploader .el-upload {\r\n    border: 1px dashed #d9d9d9;\r\n    border-radius: 6px;\r\n    cursor: pointer;\r\n    position: relative;\r\n    overflow: hidden;\r\n  }\r\n  .avatar-uploader .el-upload:hover {\r\n    border-color: #409EFF;\r\n  }\r\n  .avatar-uploader-icon {\r\n    font-size: 28px;\r\n    color: #8c939d;\r\n    width: 178px;\r\n    height: 178px;\r\n    line-height: 178px;\r\n    text-align: center;\r\n  }\r\n  .avatar {\r\n    width: 178px;\r\n    height: 178px;\r\n    display: block;\r\n  }\r\n</style>\r\n"]}]}